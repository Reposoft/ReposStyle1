<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="/repos-web/view/repos.xsl"?>
<!DOCTYPE svn [
  <!ELEMENT svn   (index)>
  <!ATTLIST svn   version CDATA #REQUIRED
                  href    CDATA #REQUIRED>
  <!ELEMENT index (updir?, (file | dir)*)>
  <!ATTLIST index name    CDATA #IMPLIED
                  path    CDATA #IMPLIED
                  rev     CDATA #IMPLIED>
  <!ELEMENT updir EMPTY>
  <!ELEMENT file  EMPTY>
  <!ATTLIST file  name    CDATA #REQUIRED
                  href    CDATA #REQUIRED>
  <!ELEMENT dir   EMPTY>
  <!ATTLIST dir   name    CDATA #REQUIRED
                  href    CDATA #REQUIRED>
]>
<svn version="1.4.6 (r28521)"
     href="http://subversion.tigris.org/">
  <index rev="0" path="Collection of Repositories">
    <dir name="data" href="data/" />
    <dir name="training" href="training/" />
    <dir name="update" href="update/" />
  </index>
</svn>
